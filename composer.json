{
    "name": "nibbletech/expectation-lexer",
    "description": "Lexer that is configured to expect certain tokens",
    "type": "library",
    "autoload": {
        "psr-4": {
            "NibbleTech\\ExpectationLexer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NibbleTech\\ExpectationLexer\\TestHelpers\\": "tests/TestHelpers/"
        }
    },
    "require": {
        "php": "^8.1",
        "spatie/regex": "^3.1",
        "ramsey/uuid": "^4.3"
    },
    "require-dev": {
        "vimeo/psalm": "^4.23",
        "kint-php/kint": "^4.1",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.8",
        "psalm/plugin-phpunit": "^0.16.1",
        "roave/infection-static-analysis-plugin": "^1.18"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
